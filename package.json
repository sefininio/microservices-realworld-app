  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "start:all": "nx run-many --target=serve --all --parallel --maxParallel=6 --with-deps",
    "build:all": "nx run-many --target=build --all --parallel --maxParallel=6 --with-deps",
    "build:docker:all": "docker build apps/feature-user -t feature-user && docker build apps/feature-tag -t feature-tag && docker build apps/feature-profile -t feature-profile && docker build apps/feature-article -t feature-article && docker build apps/realworld-example-app -t gql-gateway",
    "build:docker:clean": "docker builder prune && docker image prune",
    "predeploy:clean": "yarn build:docker:clean && docker-compose down && docker image rm $(docker image ls -aq)",
    "predeploy": "yarn build:all --prod && yarn build:docker:all",
    "build:docker:deploy": "./deploy.sh",
    "dockerhub:build-deploy": "yarn predeploy && yarn build:docker:deploy",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "lint:all": "nx run-many --target=lint --all --with-deps",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
